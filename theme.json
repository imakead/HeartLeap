{
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "1000px",
			"wideSize": "1200px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
