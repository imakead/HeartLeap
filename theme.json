					"slug": "system-font"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Zen Kaku Gothic New\"",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/zen-kaku-gothic-new/zen-kaku-gothic-new-500-normal.woff2"
							]
						},
						{
							"fontFamily": "\"Zen Kaku Gothic New\"",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/zen-kaku-gothic-new/zen-kaku-gothic-new-700-normal.woff2"
							]
						}
					],
					"fontFamily": "\"Zen Kaku Gothic New\", sans-serif",
					"name": "Zen Kaku Gothic New",
					"slug": "zen-kaku-gothic-new"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Lato",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/lato/lato-700-normal.woff2"
							]
						}
					],
					"fontFamily": "Lato, sans-serif",
					"name": "Lato",
					"slug": "lato"
				},
				{
					"fontFace": [
						{
							"fontFamily": "\"Montserrat Alternates\"",
							"fontStyle": "normal",
							"fontWeight": "600",
							"src": [
								"file:./assets/fonts/montserrat-alternates/montserrat-alternates-600-normal.woff2"
							]
						}
					],
					"fontFamily": "\"Montserrat Alternates\", sans-serif",
					"name": "Montserrat Alternates",
					"slug": "montserrat-alternates"
				}
			],
			"fontSizes": []
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"blocks": {
			"core/button": {
				"css": "  display: inline-block;\n  min-width: 228px;\n  font-size: 16px;\n  font-weight: 700;\n  color: var(--white-text);\n  text-align: center;\n  padding: 20px 10px;\n  background-color: var(--main-text);\n  border-radius: 4px;\n"
			}
		},
		"color": {
			"background": "#ffffffba",
			"text": "#333333"
		},
		"elements": {
			"button": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--zen-kaku-gothic-new)"
				}
			},
			"caption": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--zen-kaku-gothic-new)"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--zen-kaku-gothic-new)",
					"fontSize": "36px",
					"fontStyle": "normal",
					"fontWeight": "700",
					"lineHeight": "1.6"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--zen-kaku-gothic-new)",
					"fontSize": "40px",
					"lineHeight": "1.6"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--zen-kaku-gothic-new)",
					"fontSize": "20px",
					"lineHeight": "1"
				}
			},
			"h4": {
				"typography": {
					"fontSize": "16px"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--zen-kaku-gothic-new)",
					"fontStyle": "normal",
					"fontWeight": "500"
				}
			},
			"link": {
				"color": {
					"text": "#333333"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--zen-kaku-gothic-new)"
				}
			}
		},
		"spacing": {
			"padding": {
				"bottom": "0px",
				"left": "16px",
				"right": "16px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--zen-kaku-gothic-new)",
			"fontSize": "16px",
			"fontStyle": "normal",
			"fontWeight": "500",
			"lineHeight": "1.6"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json"
}